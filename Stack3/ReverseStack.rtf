void reverse(Stack s){
	if(s.isEmpty()) return;
	int pop=s.pop();
	reverse(s);
	insertAtBottom(s,pop);
}
insertAtBottom(Stack s,int val){
	if(s.isEmpty()){
		s.push(val);
		return;
	}
	int pop=s.pop();
	insertAtBoottom(s,val);
	s.push(pop);
}